import javax.swing.*;
import java.awt.*;
import java.awt.event.*;

class p123
extends InfoFrame
{
	public p123()
	{
		setSize(600,300);
		setVisible(true);
	}
	public void paint(Graphics g)
	{
		g.setColor(Color.BLUE);
		g.fillRect(50,50,100,100);
		g.setColor(Color.RED);
		g.fillOval(50,200,200,100);
	}
	public static void main(String cp[])
	{
		new p123();
	}
}
